<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Custom suite">

    <!-- name 必须Parameters注解接收名一致，value就是为参数的值-->
    <parameter name="linetype" value="${linetype}"/>
    <parameter name="start" value="${start}"/>
    <parameter name="end" value="${end}"/>
    <parameter name="jicang" value="${jicang}"/>
    <parameter name="cusclass" value="${cusclass}"/>
    <parameter name="vip" value="${vip}"/>
    <parameter name="luggage" value="${luggage}"/>
    <parameter name="ticktvalue" value="${ticktvalue}"/>
    <parameter name="res" value="${res2_3}"/>
    <test verbose="1" name="Selenium_Demo" annotations="JDK">
        <classes>
            <!-- 设置需要执行的class路径名-->
            <class name="hyg.test.TestDemo3"/>
        </classes>
    </test>
</suite>
